{
  "name": "voskobovich/yii2-base-toolkit",
  "description": "Tools for Yii2",
  "keywords": [
    "base",
    "toolkit",
    "yii2",
    "helpers"
  ],
  "homepage": "https://github.com/voskobovich/yii2-base-toolkit",
  "type": "yii2-toolkit",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/voskobovich/yii2-base-toolkit/issues",
    "source": "https://github.com/voskobovich/yii2-base-toolkit"
  },
  "authors": [
    {
      "name": "Vitaly Voskobovich",
      "email": "vitaly@voskobovich.com",
      "homepage": "http://voskobovich.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "voskobovich\\base\\db\\": "src/db",
      "voskobovich\\base\\helpers\\": "src/helpers",
      "voskobovich\\base\\forms\\": "src/forms",
      "voskobovich\\base\\traits\\": "src/traits",
      "voskobovich\\base\\widgets\\": "src/widgets",
      "voskobovich\\base\\interfaces\\": "src/interfaces"
    }
  }
}